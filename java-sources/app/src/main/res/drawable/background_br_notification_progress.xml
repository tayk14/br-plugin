<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background" android:height="10dp" android:top="-5dp" android:right="1.4dp">
        <shape>
            <gradient android:startColor="#0b131e" android:endColor="#0d1522" android:angle="270" android:centerY="1" android:centerColor="#0b131e"/>
            <corners android:bottomRightRadius="10dp"/>
        </shape>
    </item>
    <item android:id="@android:id/progress" android:height="10dp" android:top="-5dp" android:right="1.4dp">
        <clip>
            <shape android:shape="rectangle">
                <gradient android:startColor="#ff6317" android:endColor="#c50000" android:angle="180" android:type="linear"/>
                <corners android:bottomRightRadius="10dp"/>
            </shape>
        </clip>
    </item>
</layer-list>
